{
  "manifest_version": 2,
  "author": "Thomas Burette",
  "name": "Game About Squares Solver",
  "version": "1.0",
  "description": "Help solving levels for the game gameaboutsquares.com",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png" },
  "content_scripts": [
      {
